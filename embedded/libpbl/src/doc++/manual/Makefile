html:	doc.dxx
	case $(LANG) in ja_JP.EUC | ja_JP.ujis | ja) \
		export LANG=$(LANG);\
		$(DOCXX) -d html $(srcdir)/doc.dxx ;;\
	*)  $(DOCXX) -d html $(srcdir)/doc.dxx ;;\
	esac;

