latex:	doc.dxx docxx-br.sty docxx-ja.sty docxx-ro.sty docxx.sty
	case $(LANG) in ja_JP.EUC | ja_JP.ujis | ja) \
		export LANG=$(LANG);\
		$(DOCXX) -t -o doc.tex -ep a4wide $(srcdir)/doc.dxx;\
		latex doc;\
		latex doc;;\
	*) $(DOCXX) -t -o doc.tex -ep a4wide $(srcdir)/doc.dxx;\
		latex doc;\
		latex doc;;\
	esac;

ps:
	$(MAKE) latex
	dvips -o doc.ps doc.dvi

pdf:
	$(MAKE) ps
	ps2pdf doc.ps

